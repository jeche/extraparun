<head>
<!--[if lt IE 9]>
<script src="js/html5.js"></script>
<link rel="stylesheet" type="text/css" media="screen" href="css/ie.css">
<![endif]-->
</head>
<body>
<div class="main">
<div class="bg-img"></div>
<!--==============================header=================================-->
<header>
<h1><a href="#"><span class="page-header">Welcome to</span><strong>Extraparun!</strong></a></h1>
</header>
<!--==============================content================================-->
<section id="content">
    <div class="container_12">
	<div class="grid_12 top-1">
	<div class="center"><strong>Please sign in:</strong></div>
        <div id="signin-form">
			<%= form_for(:session, url: sessions_path) do |f| %>
				<div>
				  <%= f.label :email %>
				  <%= f.text_field :email %>
				</div>
				<div>
				  <%= f.label :password %>
				  <%= f.password_field :password %>
				</div>
				  
			<% end %>
		</div>		
		<div class="center"><a href="/welcome" class="search_button">Sign In</a></div>	
		<div class="center">New user? <%= link_to "Sign up now!", signup_path, :class=>"link"%></div>		
	</div>
	</div>
</section>
<div class="clear"></div>
 <!--==============================footer=================================-->
  <footer>
    <p>2014 Extraparun</p> 
	<p>Created By: Blohm, Chen, Jones, Torres</p>
    <p>Website Template by <a target="_blank" href="http://www.templatemonster.com/" class="link">TemplateMonster.com</a></p>
  </footer>
</div>
</body>