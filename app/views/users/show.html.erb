<% @prepage = "My" %>
<% @page = "Profile" %>
<section id="content">
    <div class="container_12">
	<div class="grid_12 top-1">
        <div class="block-1 box-shadow">
			<p id="notice"><%= notice %></p>

			<p>
			  <strong>Name:</strong>
			  <%= @user.name %>
			</p>

			<p>
			  <strong>Email:</strong>
			  <%= @user.email %>
			</p>
			<%= link_to 'Edit', edit_user_path(@user), class:"search_button" %>
		</div>
	</div>
	</div>
</section>